{
  "name": "@runanywhere/web",
  "version": "0.1.0",
  "description": "RunAnywhere Web SDK - On-device AI inference in the browser via RACommons WASM",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./wasm/*": "./wasm/*"
  },
  "files": [
    "dist/",
    "wasm/",

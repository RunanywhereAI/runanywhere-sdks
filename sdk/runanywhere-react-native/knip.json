{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/index.ts"
  ],
  "project": [
    "src/**/*.ts",
    "!src/**/__tests__/**",
    "!src/**/*.test.ts"
  ],
  "ignore": [
    "lib/**",
    "nitrogen/**",
    "node_modules/**",
    "android/**",
    "ios/**",
    "cpp/**",
    "example/**",
    "src/__tests__/**",
    "src/**/*.test.ts"
  ],
  "ignoreDependencies": [
    "eslint-plugin-jest",
    "eslint-plugin-prettier",
    "@react-native-async-storage/async-storage",
    "rn-fetch-blob",
    "react-native-fs",
    "@commitlint/*",
    "@evilmartians/*",
    "commitlint"
  ],
  "rules": {
    "files": "error",
    "exports": "warn",
    "types": "warn",
    "unlisted": "warn",
    "binaries": "warn",
    "unresolved": "warn"
  }
}

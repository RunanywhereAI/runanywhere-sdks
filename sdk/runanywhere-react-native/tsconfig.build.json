{
  "extends": "./tsconfig",
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "outDir": "lib/typescript",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "strict": false
  },
  "include": ["src"],
  "exclude": [
    "src/services/AuthenticationService.ts",
    "src/services/ConfigurationService.ts",
    "src/services/DownloadService.ts",
    "src/services/ModelRegistry.ts"
  ]
}
